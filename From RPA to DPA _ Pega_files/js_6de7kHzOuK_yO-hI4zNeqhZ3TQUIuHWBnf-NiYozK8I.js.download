      let lmms = window.lmms;
      self.pollAttempts++;

      if (self.pollAttempts > self.maxPollAttempts) {
        clearInterval(self.interval);
        lmms.logger('LMMS Drift Tracking: Drift object not available. Aborting tracking.', 'log');
        return;
      }

      lmms.logger('LMMS Drift Tracking: Checking if Drift object has loaded.');
      if (window.drift) {
        self.getCsrfToken();
        window.drift.on('ready', function (api, payload) {
          self.loadEvents(api, payload);
        });
        clearInterval(self.interval);
        lmms.logger('LMMS Drift Tracking: Tracking events initialized.')
      }
    },

    /**
     * Get CSRF Token.
     *
     * @see https://www.drupal.org/docs/8/core/modules/rest/3-post-for-creating-content-entities
     *
     * @return {Promise} Promise object that will return the CSRF token if successful.
     */
    getCsrfToken: function () {
      $.get(Drupal.url('session/token'))
        .done(function (data) {
          Drupal.lmmsDriftTracking.csrfToken = data;
        });
    },

    /**
     * Events loader.
     *
     * @param api
     * @param payload
     */
    loadEvents(api, payload) {
      let self = this;
      let availableEvents = [
        'attachInvitation',
        'attachSchedulingMeetingBooked',
        'attachConversation',
        'attachEmailCapture',
        'attachPhoneCapture',
      ];

      for (const eventId in availableEvents) {
        let eventName = availableEvents[eventId] || null;

        if (eventName) {
          self.events[eventName](self, api, payload);
        }
      }
    },

    /**
     * Send LMMS Event.
