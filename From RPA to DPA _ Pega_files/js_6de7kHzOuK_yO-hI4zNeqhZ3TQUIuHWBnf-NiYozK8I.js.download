      }

      return false;
    },
    fireFormImpressedEvent: function (form) {
      var isFormVisible = Drupal.pegaWebformImpressionStateEvents.isInViewport(form);
      var isEventImpressionFired = Drupal.pegaWebformImpressionStateEvents.wasImpressedEventFired(form);
      if (isFormVisible && !isEventImpressionFired) {
        var sessionKey = 'impressionForm_' + form.id;
        sessionStorage.setItem(sessionKey, '1');

        var e = new CustomEvent('pega_webform_impression');
        window.dispatchEvent(e);
      }
    },
  };

  Drupal.behaviors.pega_webform_impression_state_events = {
    attach: function (context, settings) {
      if (context !== document) {
        return;
      }

      window.addEventListener('load', function (event) {
        var forms = $('.webform-submission-form', context);
        if (forms.length) {
          var modal = document.querySelector('bolt-modal');
          if (modal) {
            modal.addEventListener('modal:show', function () {
              Drupal.pegaWebformImpressionStateEvents.fireFormImpressedEvent(forms[0]);
            });
          }

          var sessionKey = 'impressionForm_' + forms[0].id;
          if (sessionStorage.getItem(sessionKey)) {
            sessionStorage.removeItem(sessionKey);
          }
          var in_modal = $(forms[0]).parents('bolt-modal');
          if (in_modal.length === 0) {
            Drupal.pegaWebformImpressionStateEvents.fireFormImpressedEvent(forms[0]);
          }
        }
      });

      window.addEventListener('scroll', function (event) {
        var forms = $('.webform-submission-form', context);
        if (forms.length) {
          var in_modal = $(forms[0]).parents('bolt-modal');
          var confirm = $(forms[0]).find('.webform-confirmation', context);
          if (confirm.length === 0 && in_modal.length === 0) {
