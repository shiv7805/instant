          // TODO Switch to client-side, use Drift webhooks conversation_inactive event.
          // @see https://devdocs.drift.com/docs/webhook-events-1#event-types
          // @see https://atendesigngroup.com/blog/capturing-webhooks-in-drupal-8
          let time;
          let idled = false;
          let playbookId = event.playbookId || Drupal.lmmsDriftTracking.playbookId;
          let conversationId = event.conversationId;
          window.onload = resetTimer;
          // DOM Events
          document.onmousemove = resetTimer;
          document.onkeypress = resetTimer;

          function idle() {
            scope.sendLmmsEvent({
              ACTIVITY_TYPE: 'Chatbot',
              ACTIVITY_SUB_TYPE: 'Conversation',
              ASSET_ID: playbookId,
              DESCRIPTION: conversationId,
            });
            idled = true;
          }

          function resetTimer() {
            clearTimeout(time);
            // 10 minutes
            time = setTimeout(idle, 600000)
          }

          // sendLmmsEvent on window close.
          window.addEventListener('beforeunload', function(event) {
            if (!idled) {
              scope.sendLmmsEvent({
                ACTIVITY_TYPE: 'Chatbot',
                ACTIVITY_SUB_TYPE: 'Conversation',
                ASSET_ID: playbookId,
                DESCRIPTION: conversationId,
              });
            }
            return false;
          });
        });
      },

