/**
 * Track Drift events in LMMS.
 *
 * @see https://gitlab.com/pegadigital/pm/pega_analytics/-/blob/22b179733db39120b8643a389e55f4ef22420f66/js/drift-track.js
 */
(function (window, $, Drupal, drupalSettings) {
  'use strict';

  Drupal.lmmsDriftTracking = {
    email: '',
    phone: '',
    playbookId: '',
    convoTracked: false,
    csrfToken: '',
    interval: false,
    pollAttempts: 0,
    maxPollAttempts: 30,
    frequency: 500,

    /**
     * @inheritDoc
     */
